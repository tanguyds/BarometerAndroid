<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.nalu.barometer.dashboard.DashboardActivity">

    <ListView
        android:id="@+id/lvDashboard"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="192dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:listSelector="@color/colorNalu"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvStringDashboard" />

    <TextView
        android:id="@+id/tvStringDashboard"
        android:layout_width="wrap_content"
        android:layout_height="41dp"
        android:layout_marginBottom="2dp"
        android:layout_marginTop="6dp"
        android:text="TextView"
        android:textColor="@color/colorNalu"
        android:textSize="25dp"
        app:layout_constraintBottom_toTopOf="@+id/lvDashboard"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</android.support.constraint.ConstraintLayout>
